<% provide(:title, "Video Session") %>
<h1>Video Session</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2 col-xs-12">
    <div class="form-group">
      <%= button_to 'Leave Callback', edit_callback_video_session_path(@video_session), method: :get, class: "btn btn-primary" %>
    </div>
    <div class="form-group">
      <%= button_to 'Leave Message', edit_message_video_session_path(@video_session), method: :get, class: "btn btn-primary" %>
    </div>
    <div class="form-group">
      <%= button_to 'End Session', edit_feedback_video_session_path(@video_session), method: :get, class: "btn btn-primary" %>
    </div>
  </div>
</div>
